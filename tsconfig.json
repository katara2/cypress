  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["cypress", "node"],
    "lib": ["ES6", "DOM"]
  },
  "include": ["cypress/**/*.ts"],
  "exclude": ["node_modules"]
}


  
  